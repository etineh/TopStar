<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/cardTopOrange"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:elevation="2dp">

    <ImageView
        android:id="@+id/backArrow_IV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:padding="7dp"
        app:layout_constraintBottom_toBottomOf="@+id/circleImageLogo9"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/circleImageLogo9"
        app:srcCompat="@drawable/baseline_arrow_back_24"
        app:tint="@color/white" />

    <TextView
        android:id="@+id/userName_TV"
        style="@style/textColorFullBlack"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="2dp"
        android:ellipsize="end"
        android:maxLines="1"
        android:text="Topperian"
        android:textColor="@color/white"
        android:textSize="17sp"
        app:layout_constraintEnd_toStartOf="@+id/gift_IV"
        app:layout_constraintStart_toEndOf="@+id/circleImageLogo9"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/gift_IV"
        style="@style/tintWhiteAndCoolOrange"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="9dp"
        android:padding="7sp"
        app:layout_constraintEnd_toStartOf="@+id/callMeet_IV"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/money_round" />

    <ImageView
        android:id="@+id/callMeet_IV"
        style="@style/tintWhiteAndCoolOrange"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="9dp"
        android:padding="7sp"
        app:layout_constraintEnd_toStartOf="@+id/chatMenu_IV"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/baseline_add_ic_call_24" />

    <ImageView
        android:id="@+id/chatMenu_IV"
        android:layout_width="30dp"
        android:layout_height="35dp"
        android:layout_marginEnd="5sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/baseline_more_vert_24"
        app:tint="@color/white" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/circleImageLogo9"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="5dp"
        android:src="@drawable/logo"
        app:layout_constraintStart_toEndOf="@+id/backArrow_IV"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/onlineStatus_TV"
        style="@style/textColorWhiteAndCoolOrange"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="-2dp"
        android:text="@string/app_name"
        android:textSize="11sp"
        app:layout_constraintStart_toStartOf="@+id/userName_TV"
        app:layout_constraintTop_toBottomOf="@+id/userName_TV" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/line_const"
        style="@style/Background"
        android:layout_width="1dp"
        android:layout_height="0dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/onlineStatus_TV"
        app:layout_constraintTop_toBottomOf="@+id/userName_TV">

    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/bioHint_TV"
        style="@style/roundButtonBackground"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="4dp"
        android:ellipsize="end"
        android:gravity="center"
        android:maxLines="2"
        android:paddingBottom="2dp"
        android:text="@string/hint2"
        android:textColor="@color/defaultWhite"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/circleImageLogo9"
        app:layout_constraintTop_toBottomOf="@+id/onlineStatus_TV" />

    <TextView
        android:id="@+id/isTyping_TV"
        style="@style/textColorWhiteAndCoolOrange"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="11sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toBottomOf="@+id/onlineStatus_TV"
        app:layout_constraintEnd_toStartOf="@+id/gift_IV"
        app:layout_constraintStart_toEndOf="@+id/onlineStatus_TV"
        app:layout_constraintTop_toTopOf="@+id/onlineStatus_TV" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/line2"
        style="@style/BackgroundB_W"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:visibility="gone"
        app:layout_constraintBottom_toTopOf="@+id/bioHint_TV"
        app:layout_constraintEnd_toEndOf="@+id/bioHint_TV"
        app:layout_constraintStart_toStartOf="@+id/circleImageLogo9">

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>